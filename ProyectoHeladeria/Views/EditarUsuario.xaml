<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoHeladeria.Views.EditarUsuario">
    <ContentPage.Content>
        <StackLayout>
            <Frame>
                <Label Text="Editar Usuario" />
            </Frame>
            
            <Entry x:Name="entIdUsuario" />
            <Entry x:Name="entImagenUsuario" />
            <Entry x:Name="entCedulaUsuario" />
            <Entry x:Name="entNombresUsuario" />
            <Entry x:Name="entApellidosUsuario" />
            <Entry x:Name="entTelefonoUsuario" />
            <Entry x:Name="entDireccionUsuario" />
            <Entry x:Name="entCorreoUsuario" />
            
            <Entry x:Name="entUsuario" />
            <Entry x:Name="entContraseñaUsuario" />
            <Entry x:Name="entEstadoUsuario" />
            <Entry x:Name="entPerfilUsuario" />

            <Button x:Name="btnGuardar" Clicked="btnGuardar_Clicked" Text="Guardar" />
            <Button x:Name="btnCancelar"/>





        </StackLayout>
    </ContentPage.Content>
</ContentPage>